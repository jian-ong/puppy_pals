<h1>Edit me</h1>


<form action="/dogs/<%= dog['id'] %>" method ="post">
    <input type="hidden" name= "_method" value="patch">
    <label for="">Name</label>
    <input type="text" name="name" value="<%= dog["username"]%>"> 
    <br>
    <br>
    <label for="">Image URL</label>
    <input type="text" name ="image_url" value="<%= dog["image_url"]%>">
    <br>
    <br>
    <label for="">Age</label>
    <input type="text" name ="age" value="<%= dog["age"]%>">
    <br>
    <br>
    <label for="">Gender</label>
    <input type="text" name ="gender" value="<%= dog["gender"]%>">
    <br>
    <br>
    <label for="">Breed</label>
    <input type="text" name ="breed" value="<%= dog["breed"]%>">
    <br>
    <br>
    <label for="">Bio</label>
    <input type="text" name ="bio" value="<%= dog["bio"]%>">
    <br>
    <br>
    <label for="">Location (Suburb)</label>
    <input type="text" name ="loc_suburb" value="<%= dog["loc_suburb"]%>">
    <br>
    <br>
    <label for="">Location (State)</label>
    <input type="text" name ="loc_state" value="<%= dog["loc_state"]%>">
    <br>
    <br>
    <label for="">Location(Country)</label>
    <input type="text" name ="loc_country" value="<%= dog["loc_country"] %>">
    <br>
    <br>
    <button>Submit</button>

</form>
